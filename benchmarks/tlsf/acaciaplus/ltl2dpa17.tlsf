INFO {
  TITLE:       "LTL -> DPA  -  Example 17  (Source: Acacia+)"
  DESCRIPTION: "DPA A with 2 priorities: a word u is accepted by A if the minimal priority seen infinitely often is even"
  SEMANTICS:   Moore
  TARGET:      Mealy
}

MAIN {

  INPUTS {
    a;
    b;
    c;
  }

  OUTPUTS {
    p0;
    p1;
  }

  INVARIANTS {
    p0 <-> !p1;
  }

  GUARANTEES {
    ((G (b && G F a) && G (c || G F !a)) || G b || G c)
      <-> G F p0;
  }

}

